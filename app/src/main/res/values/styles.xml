<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ActionBarTheme" parent="Theme.AppCompat.NoActionBar">
    <item name="windowActionBar">false</item>
    <item name="android:windowNoTitle">true</item>
    </style>
    <style name="MyTabLayoutTextAppearance" parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textSize">26dp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="Home_View">
        <item name="android:layout_width">240dp</item>
        <item name="android:background">#efefef</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
    <style name="sens_View">
        <item name="android:layout_width">240dp</item>
        <item name="android:background">#efefef</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_gravity">center</item>
    </style>
</resources>
